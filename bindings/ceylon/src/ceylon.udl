namespace ceylon {
  string version();
  void enable_log();
};

dictionary WorkSpaceConfig{
    string name;
    u16 port = 8888;
};

interface WorkSpace{
    constructor(WorkSpaceConfig config);

    [Async]
    void run(bytes inputs);
};

dictionary AdminAgentConfig {
    string name;
    u16 port;
};


interface AdminAgent{
    constructor(AdminAgentConfig config);
    [Async]
    void run(bytes inputs);
};